{
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/assets/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=31536000"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "max-age=31536000"
        }
      ]
    }
  ]
} 